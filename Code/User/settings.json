{
    "workbench.startupEditor": "none",
    "workbench.secondarySideBar.defaultVisibility": "hidden",
    "workbench.colorTheme": "Catppuccin Frappé",
    "workbench.iconTheme": "catppuccin-frappe",
    "editor.fontFamily": "'FiraCode Nerd Font Mono', monospace",
    "git.autofetch": true,
    "git.enableSmartCommit": false,
    "git.suggestSmartCommit": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "workbench.tree.renderIndentGuides": "always",
    "indentRainbow.colors": [
        "rgba(0,0,0,0.0)",
        "rgba(0,0,0,0.0)",
        "rgba(0,0,0,0.0)",
        "rgba(255,0,0,0.125)"
    ],
    "indentRainbow.errorColor": "rgba(255,0,0,0.125)",
    "indentRainbow.tabmixColor": "rgba(255,0,0,0.125)",
    "makefile.configureOnOpen": true,
    "editor.guides.bracketPairs": true,
    "editor.fontLigatures": true,
    "git.confirmSync": false,
    "indentRainbow.includedLanguages": [
        "rust",
        "zig",
        "c",
        "c++"
    ],
    "claudeCode.preferredLocation": "panel",
    "gitlens.showWhatsNewAfterUpgrades": false,
    "gitlens.plusFeatures.enabled": false,
    "cSpell.language": "en, en-GB",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "diffEditor.ignoreTrimWhitespace": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "chat.agent.enabled": false,
    "git.openRepositoryInParentFolders": "always",
    "terminal.integrated.initialHint": false,
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "auto-comment-blocks.singleLineBlockOnEnter": true,
    "auto-comment-blocks.slashStyleBlocks": [
        "Rust"
    ],
    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#f4b8e4",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        },
        {
            "tag": "?",
            "color": "#8caaee",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "todo",
            "color": "#ef9f76",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "*",
            "color": "#a6d189",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        }
    ],
    "rust-analyzer.check.command": "clippy",
    "errorLens.margin": "8ch",
    "errorLens.messageTemplate": "$severity [$count] $message",
    "errorLens.padding": "0ch 1ch 0ch 1ch",
    "errorLens.borderRadius": "0.5ch",
    "errorLens.messageBackgroundMode": "message",
    "errorLens.severityText": [
        "",
        "",
        "",
        ""
    ],
    "errorLens.border": [
        "1px solid",
        "1px solid",
        "1px solid",
        "1px solid"
    ],
    "errorLens.enabledDiagnosticLevels": [
        "error",
        "warning",
        "info",
        "hint"
    ],
    "errorLens.gutterEmoji": {
        "error": "",
        "warning": "",
        "info": "",
        "hint": ""
    }
}